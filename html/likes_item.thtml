<div id="likes_item_@_ITEM_ID_@">
   <img src="@_IMAGE_URL_@" class="clickable" onclick="document.getElementById('likes_to_playlist_button_@_ITEM_ID_@').click()">
   <button id="likes_to_playlist_button_@_ITEM_ID_@" name="likes" value="@_ITEM_ID_@" class="add_playlist_buttons" onclick="addToPlaylist(this)">@_VIDEO_TITLE_@</button>
   <button name="likes" value="@_ITEM_ID_@" class="like_buttons" onclick="addRemoveLike(this)"><i class="fa@_LIKE_@ fa-heart"></i></button>
</div>
