<div songId="@_SONG_ID_@">
  <img src="@_SONG_THUMBNAIL_LINK_@" class="clickable" onclick="addToPlaylist(this)" title="Add song to the playlist">
  <button class="add_playlist_buttons" onclick="addToPlaylist(this)" title="Add song to the playlist">@_SONG_TITLE_@</button>
  <button class="like_buttons" onclick="addRemoveLike(this)" title="Like/Unlike song"><i class="fa@_LIKED_@ fa-heart"></i></button>
</div>
